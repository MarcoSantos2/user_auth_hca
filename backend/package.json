{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend of the Health Care Aide App",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "generate:secret": "ts-node src/scripts/generateRandomSecret.ts",
    "decode:jwt": "ts-node src/scripts/decodeJWT.ts",
    "test": "",
    "migrations": "npx typeorm-ts-node-commonjs migration:run -d ./src/datasource.ts",
    "migration:create": "npx typeorm migration:create",
    "smile": "ts-node src/scripts/smile.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cxfos/health_care_aide_app.git"
  },
  "author": "Felipe dos Santos and Marco dos Santos",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cxfos/health_care_aide_app/issues"
  },
  "homepage": "https://github.com/cxfos/health_care_aide_app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "google-auth-library": "^9.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
